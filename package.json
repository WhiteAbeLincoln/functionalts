{
  "name": "functionalts",
  "version": "0.0.1",
  "description": "Functional (Haskell style) programming in typescript",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "tsc -p ./tsconfig.build.json",
    "clean": "rm -r build",
    "cov": "run-s cov:html && xdg-open coverage/index.html",
    "cov:html": "jest --coverage --coverageReporters='html'",
    "cov:check": "jest --coverage"
  },
  "keywords": [
    "functional",
    "typescript",
    "higher",
    "kinded",
    "types"
  ],
  "author": "Abraham White <abelincoln.white@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.12",
    "fast-check": "^1.10.0",
    "jest": "^23.6.0",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  }
}
